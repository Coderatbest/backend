{
  "httpPort": 5488,
  "store": {
    "provider": "fs"  
  },
  "blobStorage": {
    "provider": "memory"
  },
  "logger": {
    "console": {
      "transport": "console",
      "level": "debug"
    }
  },
  "allowLocalFilesAccess": true,
  "reportTimeout": 60000,
  "workers": {
    "numberOfWorkers": 2
  },
  "extensions": {
    "fs-store": {
      "dataDirectory": "/app/data"
    },
    "authentication": {
      "cookieSession": {},
      "admin": {
        "username": "admin",
        "password": "password"
      },
      "enabled": false
    }
  }
}